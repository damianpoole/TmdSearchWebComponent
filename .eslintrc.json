{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "prettier/react"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
          }
    },
    "rules": {
        "prettier/prettier": {
            "indent": [
                "error",
                4
            ]
        }
    }
}